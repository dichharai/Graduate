ery .gallery_wrapper{\n    max-width:748px;\n    width: 100%;\n   }\n\n\n.galleria-sidebar{\n\t position: absolute;\n\t left: 660px;\n\t top: 0px;\n\t width: 300px;\n\t height: 510px;\n         background: #3c3d3d;\n}\n\n#multimedia_main_gallery .galleria-sidebar{\n\t left: 0px;\n\t top: 520px;\n\t max-width: 620px;\n\t height: 67px;\n   color: #363636;\n   width: 100%;\n   background: transparent;\n}\n\n.galleria-ad {\n\t width: 300px;\n\t height: 250px;\n         overflow: hidden;\n}\n\n#multimedia_main_gallery .galleria-ad {\n display: none;\n}\n\n#galleria-loader{height:1px!important}\n\n.galleria-container {\n    position: relative;\n    /*overflow: hidden;*/\n    background: #1c1c1c;\n    -moz-border-radius-bottomright: 5px;\n    border-bottom-right-radius: 5px;\n    -moz-border-radius-bottomleft: 5px;\n    border-bottom-left-radius: 5px;\n}\n.galleria-container img {\n    -moz-user-select: none;\n    -webkit-user-select: none;\n    -o-user-select: none;\n}\n.galleria-stage {\n    position: absolute;\n    top: 0px;\n    bottom: 80px;\n    left: 0px;\n    right: 0px;\n    overflow:hidden;\n}\n.galleria-thumbnails-container {\n    height: 80px;\n    bottom: 0;\n    position: absolute;\n    left: 0;\n    right: 0;\n    z-index: 2;\n}\n.galleria-carousel .galleria-thumbnails-list {\n    margin-left: 65px;\n    margin-right: 65px;\n    margin-top: 10px;\n}\n.galleria-thumbnails .galleria-image {\n    height: 60px;\n    width: 60px;\n    background: #000;\n    margin: 0 5px 0 0;\n    border: 1px solid #000;\n    float: left;\n    cursor: pointer;\n}\n.galleria-counter {\n    float: left;\n    color: #fff;\n    font-family: \"Benton Sans\",sans-serif;\n    font-size: 14px;\n    line-height: 1.333em;\n    text-align: center;\n    padding: 5px 0px;\n    z-index: 1;\n    margin-right: 15px;\n    min-width: 75px;\n}\n\n.galleria-parts {\n    width: 100%;\n    color: #fff;\n    font-family: \"Benton Sans\", Arial, sans-serif;\n    font-size: 16px;\n    z-index: 1;\n    height: 30px;\n}\n\n#multimedia_main_gallery .galleria-parts, #multimedia_main_gallery .galleria-partlinks {\n    display: none;\n}\n\n.galleria-partlinks {\n height: 20px;\n}\n\n.galleria-partlinks a:link, .galleria-partlinks a:visited {\n color: #A1A2A2;\n}\n\n.galleria-partnext {\n float: right;\n}\n\n.galleria-partprev {\n float: left;\n}\n\n#multimedia_main_gallery .galleria-counter {\n    color: #363636;\n}\n\n.galleria-loader {\n    background: #000;\n    width: 20px;\n    height: 20px;\n    position: absolute;\n    top: 10px;\n    right: 10px;\n    z-index: 2;\n    display: none;\n    background: url(/static/common/js/galleria/themes/classic/classic-loader.gif) no-repeat 2px 2px;\n}\n.galleria-info {\n    width: 100%;\n    /*top: 15px;\n    left: 15px;*/\n    z-index: 2;\n    position: relative;/*absolute;*/\n    height: 260px;\n    margin-bottom: 20px;\n}\n\n.galleria-info-text {\n    /*display: none;*/\n    /* IE7 */ zoom:1;\n    height: 260px;\n    overflow: auto;\n}\n\n#multimedia_main_gallery .galleria-info-text, #multimedia_main_gallery .galleria-info{\n height: 75px;\n}\n\n.galleria-info-text a:link, .galleria-info-text a:active, .galleria-info-text a:visited {\n    color: #c5c5c5;\n    text-decoration: underline;\n    font-weight: normal;\n}\n\n.galleria-info-text a:hover {\n    color: #0088ee;\n}\n\n.galleria-info-text .galleria-info-credit a:link, .galleria-info-text .galleria-info-credit a:visited, .galleria-info-text .galleria-info-credit a:active {\n    color: #c5c5c5;\n    text-decoration: underline;\n    font-weight: normal;\n}\n\n .galleria-info-text .galleria-info-credit a:hover {\n    color: #0088ee;\n}\n\n.galleria-info-title {\n    font-family: \"Benton Sans\",sans-serif;\n    font-size: 18px;\n    margin: 0;\n    color: #f5f5f5;\n    margin: 0 0 7px 0;\n}\n\n.galleria-info-text .galleria-info-title a:visited, .galleria-info-text .galleria-info-title a:link {\n    color: #F5F5F5;\n    text-decoration: underline;\n}\n\n.galleria-info-text .galleria-info-title a:hover {\n    color: #0088ee;\n}\n\n#multimedia_main_gallery .galleria-info-title {\n       display: none;\n}\n\n.galleria-info-description {\n    font-family: \"Benton Sans\", sans-serif;\n    font-size: 14px;\n    margin: 0;\n    color: #f5f5f5;\n    overflow: hidden;\n}\n\n#multimedia_main_gallery .galleria-info-description {\n color: #333;\n font-size: 14px;\n padding: 4px 10px 15px 0px;\n line-height: 1.4em;\n letter-spacing: 0.01em;\n}\n\n.galleria-info-credit {\n    font-family: \"Benton Sans\";\n    font-size: 12px;\n    padding-top: 10px;\n    height: 20px;\n}\n\n#multimedia_main_gallery .galleria-info-credit a:link, #multimedia_main_gallery  .galleria-info-text a:link {\n\tcolor: #444;\n}\n\n#multimedia_main_gallery .galleria-info-credit a:hover, #multimedia_main_gallery  .galleria-info-text a:hover  {\n    color: #0088ee;\n}\n\n#multimedia_main_gallery .galleria-info-credit {\n /*font-style: italic;*/\n font-size: 12px;\n}\n\n.galleria-info-close {\n    width: 9px;\n    height: 9px;\n    position: absolute;\n    top: 5px;\n    right: 5px;\n    background-position: -753px -11px;\n    opacity: .5;\n    filter: alpha(opacity=50);\n    cursor: pointer;\n    display: none;\n}\n.notouch .galleria-info-close:hover{\n    opacity:1;\n    filter: alpha(opacity=100);\n}\n.touch .galleria-info-close:active{\n    opacity:1;\n    filter: alpha(opacity=100);\n}\n.galleria-info-link {\n    background-position: -669px -5px;\n    opacity: .7;\n    filter: alpha(opacity=70);\n    position: absolute;\n    width: 20px;\n    height: 20px;\n    cursor: pointer;\n    background-color: #000;\n}\n.notouch .galleria-info-link:hover {\n    opacity: 1;\n    filter: alpha(opacity=100);\n}\n.touch .galleria-info-link:active {\n    opacity: 1;\n    filter: alpha(opacity=100);\n}\n.galleria-image-nav {\n    position: absolute;\n    top: 50%;\n    margin-top: -40px;\n    width: 100%;\n    height: 80px;\n    left: 0;\n}\n.galleria-image-nav-left, .galleria-image-nav-right {\n    opacity: .3;\n    filter: alpha(opacity=30);\n    cursor: pointer;\n    width: 66px;\n    height: 80px;\n    position: absolute;\n    left: 0px;\n    z-index: 2;\n    background-position: 0 0;\n}\n.galleria-image-nav-right {\n    left: auto;\n    right: 0px;\n    background-position: -250px 0px;\n    z-index: 2;\n}\n.notouch .galleria-image-nav-left:hover, .notouch .galleria-image-nav-right:hover {\n    opacity: 1;\n    filter: alpha(opacity=100);\n}\n.touch .galleria-image-nav-left:active, .touch .galleria-image-nav-right:active {\n    opacity: 1;\n    filter: alpha(opacity=100);\n}\n.galleria-thumb-nav-left, .galleria-thumb-nav-right {\n    cursor: pointer;\n    display: none;\n    position: absolute;\n    left: 1px;\n    top: 0px;\n    height: 60px;\n    width: 60px;\n    z-index: 3;\n    opacity: .8;\n    filter: alpha(opacity=80);\n}\n\n.galleria-thumb-nav-left {\n    background-image: url(/static/common/img/galleria/thumb_left.png);\n    background-repeat: no-repeat;\n}\n\n\n.galleria-thumb-nav-right {\n    background-image: url(/static/common/img/galleria/thumb_right.png);\n    background-repeat: no-repeat;\n    border-right: none;\n    right: 1px;\n    left: auto;\n}\n.galleria-thumbnails-container .disabled {\n    opacity: .2;\n    filter: alpha(opacity=20);\n    cursor: default;\n}\n.notouch .galleria-thumb-nav-left:hover, .notouch .galleria-thumb-nav-right:hover {\n    opacity: 1;\n    filter: alpha(opacity=100);\n    background-color: #111;\n}\n.touch .galleria-thumb-nav-left:active, .touch .galleria-thumb-nav-right:active {\n    opacity: 1;\n    filter: alpha(opacity=100);\n    background-color: #111;\n}\n.notouch .galleria-thumbnails-container .disabled:hover {\n    opacity: .2;\n    filter: alpha(opacity=20);\n    background-color: transparent;\n}\n\n.galleria-carousel .galleria-thumb-nav-left,\n.galleria-carousel .galleria-thumb-nav-right {\n    display: block;\n    margin-top: 10px;\n}\n\n.galleria-info-link,\n.galleria-info-close,\n.galleria-image-nav-left,\n.galleria-image-nav-right {\n    background-image: url(/static/common/img/galleria/classic-map.png);\n    background-repeat: no-repeat;\n}\n\n\n/*sk mucking about....*/\n.galleria-bar {\n    height: 30px;\n    position: absolute;\n    bottom: -39px;\n    left: 0;\n    right: 0;\n    z-index: 3;\n    overflow: visible;\n}\n\n#multimedia_main_gallery .galleria-bar {\n bottom: -125px;\n}\n\n.galleria-fullscreen, .galleria-play {\n    width: 40px;\n    height: 40px;\n    left: 0;\n    background-position: 50% 50%;\n    background-repeat: no-repeat;\n    cursor: pointer;\n    border-left: 1px solid #000000;\n}\n\n.galleria-fullscreen {\n    width: 99px;\n    height: 30px;\n    float: left;\n    background-image: url(/static/common/img/galleria/fullscreen.png);\n    bottom: 0px;\n    padding: 0 15px;\n}\n\n#multimedia_main_gallery .galleria-fullscreen {\n    background-image: url(/static/common/img/galleria/fullscreen_embed.png);\n    height: 28px;\n}\n\n.galleria-share {\n    height: 30px;\n    width: 71px;\n    float: left;\n    padding-left: 15px;\n    padding-right: 15px;\n    border-left: 1px solid #000000;\n}\n\n.galleria-buy {\n    height: 30px;\n    width: 58px;\n    float: left;\n    padding-left: 15px;\n    border-left: 1px solid #000000;\n}\n\n#multimedia_main_gallery .galleria-fullscreen,#multimedia_main_gallery .galleria-share,#multimedia_main_gallery .galleria-buy{\n    border-left: 1px solid #dadada;\n}\n\n.galleriaBuyLink {\n    height: 30px;\n    width: 58px;\n    float: left;\n    background: url(/static/common/img/galleria/buy.png);\n}\n\n#multimedia_main_gallery .galleriaBuyLink {\n    background: url(/static/common/img/galleria/buy_embed.png);\n    height: 28px;\n}\n\n\n\n.galleriaAlertLink {\n    height: 30px;\n    width: 60px;\n    float: left;\n    background: url(/static/common/img/galleria/alert_button.png);\n}\n\n#multimedia_main_gallery .galleriaAlertLink {\n    background: url(/static/common/img/galleria/alert_button_embed.png);\n    height: 28px;\n}\n\n\n\n   #btnShare {\n    background: url(/static/common/img/galleria/share.png);\n    height: 30px;\n    width: 71px;\n    border: 0 none;\n   }\n\n#multimedia_main_gallery #btnShare {\n    background: url(/static/common/img/galleria/share_embed.png);\n    height: 28px;\n   }\n\n#adv-network {\n height: 110px;\n}\n\n.galleria-errors {\n display: none;\n}\n\n.galleria-adOverlay {\n background: #1c1c1c;\n width: 100%;\n height: 100%;\n position: absolute;\n z-index: 400;\n left: 700px;\n display:none;\n}\n\n.galleria-adCloseLeft, .galleria-adCloseRight  {\n    cursor: pointer;\n    width: 66px;\n    height: 80px;\n    position: absolute;\n    left: 0px;\n    z-index: 2;\n    background-position: 0 0;\n    top: 50%;\n    background-image: url(/static/common/img/galleria/classic-map.png);\n    background-repeat: no-repeat;\n    margin-top: -40px;\n}\n\n\n.galleria-adCloseRight {\n    left: auto;\n    right: 0px;\n    background-position: -250px 0px;\n}\n\n.galleria-image-nav-right {\n    left: auto;\n    right: 0px;\n    background-position: -250px 0px;\n    z-index: 2;\n}\n\n#galleria_instream_ad {\n position: absolute;\n left: 50%;\n top: 50%;\n height: 250px;\n width: 300px;\n display: none;\n margin-top: -125px;\n margin-left: -150px;\n}\n\n#galleriaRelated, #galleriaLink {\n background: #1c1c1c;\n width: 620px;\n height: 408px;\n text-align: left;\n margin: auto;\n}\n\n#galleriaLink {\n text-align: center;\n}\n\n\n#galleriaRelated h1 {\n margin: 5px 10px 0px;\n width: 90%;\n color: #F5F5F5;\n font-family: \"Benton Sans\", Arial, sans-serif;\n font-size: 18px;\n}\n\n#galleriaLink h1 {\n position: relative;\n top: 50%;\n width: auto;\n margin: 0;\n font-family: \"Benton Sans\", Arial, sans-serif;\n font-size: 18px;\n margin-top: -23px;\n}\n\n#galleriaLink h1 a {\n color: #F5F5F5;\n}\n\n#galleriaRelated #related_row_one, #galleriaRelated #related_row_two {\n margin-bottom: 10px;\n width: 100%;\n\n}\n\n#galleriaRelated #related_row_two {\n margin-bottom: 0px;\n}\n\n#galleriaRelated .related_info {\n width: 178px;\n float: left;\n margin-right: 9px;\n margin-left: 17px;\n}\n\n#galleriaRelated .related_image {\n background: #262626 ;\n width: 178px;\n height: 156px;\n text-align: center;\n line-height: 156px;\n}\n\n#galleriaRelated .related_image img {\n vertical-align: middle;\n border: none;\n}\n\n#galleriaRelated p.related_title {\n color: #FFFFFF;\n font-family: Arial;\n font-size: 12px;\n margin: 5px 0 0;\n}\n\n\n#galleriaRelated p.related_title a  {\n color: #FFFFFF;\n text-decoration: none;\n}\n\n\n#galleriaRelated p.related_title a:hover {\n text-decoration: underline;\n}\n\n.galleria-fs-CaptionShow, .galleria-fs-CaptionHide {\n cursor: pointer;\n background: url(/static/common/img/galleria/Caption_button.png);\n width: 82px;\n height: 30px;\n position: relative;\n margin-left: 10px;\n}\n\n.galleria-fs-CaptionHide {\n background: url(/static/common/img/galleria/Caption_button_hide.png);\n display:none;\n}\n\n#multimedia_main_gallery .galleria-caption {\n border-left: 1px solid #dadada ;\n float: left;\n height: 28px;\n padding-left: 15px;\n padding-right: 15px;\n width: 82px;\n}\n\n#multimedia_main_gallery .galleria-embedCaptionToggle {\n cursor: pointer;\n background: url(/static/common/img/galleria/Caption_button_hide_embed.png);\n width: 82px;\n height: 28px;\n}\n\n#galleria_content .fullCaptionShow, #galleria_content.fullCaptionHide {\n display: none;\n}\n\n.galleria-fs-Close {\n cursor: pointer;\n background: url(/static/common/img/galleria/Close_button.png);\n width: 73px;\n height: 30px;\n position: absolute;\n right: 10px;\n top: 0px;\n}\n\n html.touch #galleria-AdMobileCover {\n  width: 100%;\n  height: 100%;\n  position: absolute;\n  top: 0;\n  left: 0;\n  z-index: 1;\n  cursor: pointer;\n }\n   .pages {\n    width: 960px;\n    height: 245px;\n    overflow:hidden;\n   }\n\n   .ad_page {\n    width: 300px;\n    height: 245px;\n    float: left;\n    position: relative;\n   }\n\n    .ad_page .injectedAd {\n     position: absolute;\n     left: 50%;\n     top: 50%;\n     transform: translate(-50%, -50%);\n     -ms-transform: translate(-50%, -50%);\n     -webkit-transform: translate(-50%, -50%);\n   }\n\n   #page_one {\n    background: #000000;\n    text-align: center;\n   }\n\n.spinner {\n  margin: -9px 0 0 -35px;;\n  width: 70px;\n  text-align: center;\n  position: absolute;\n  left: 50%;\n  top: 50%;\n}\n\n.spinner > div {\n  width: 18px;\n  height: 18px;\n  background-color: #FFFFFF;\n\n  border-radius: 100%;\n  display: inline-block;\n  -webkit-animation: bouncedelay 1.4s infinite ease-in-out;\n  animation: bouncedelay 1.4s infinite ease-in-out;\n  /* Prevent first frame from flickering when animation starts */\n  -webkit-animation-fill-mode: both;\n  animation-fill-mode: both;\n}\n\n.spinner .bounce1 {\n  -webkit-animation-delay: -0.32s;\n  animation-delay: -0.32s;\n}\n\n.spinner .bounce2 {\n  -webkit-animation-delay: -0.16s;\n  animation-delay: -0.16s;\n}\n\n.comment_hint {\n  font-size: 20px;\n  font-family: \"Benton Sans\", Arial, sans-serif;\n  margin: 0 0 10px 0;\n}\n\n//Fullscreen styles\n\n.fullscreen .galleria-info-title, .fullscreen .galleria-info-description {\n    color: #F5F5F5;\n}\n\n.fullscreen .galleria-info-credit {\n    color: #A1A2A2;\n}\n\n.fullscreen .galleria-info,.fullscreen .galleria-sidebar,.fullscreen .galleria-info-text{\n    height : auto;\n}\n\n.fullscreen .galleria-info-text {\n    margin : 0;\n}\n\n.fullscreen .galleria-sidebar, .fullscreen .fullCaptionShow, .fullscreen .fullClose {\n    display: block;\n}\n\n.fullscreen .galleria-sidebar, #multimedia_main_gallery .fullscreen .galleria-sidebar {\n    width:100%;\n    max-width:100%;\n    left:0px;\n    top:0px;\n    z-index:1000;\n    background:transparent;\n}\n\n.fullscreen .galleria-info {\n    margin-bottom : 0px;\n}\n\n.fullscreen .galleria-info-description, #multimedia_main_gallery .fullscreen .galleria-info-description{\n    font-size:12px;\n    background:t