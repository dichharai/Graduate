rd.teams.away.score > jsonData.scoreboard.teams.home.score) {\n                jQuery(div_id + \" .team_two .team_score, \" + div_id + \" .city.home\").addClass(\"lost\");\n            } else if (jsonData.scoreboard.teams.away.score < jsonData.scoreboard.teams.away.score) {\n                jQuery(div_id + \" .team_one .team_score, \" + div_id + \" .city.away\").addClass(\"lost\");\n            }\n        }\n        if ((jsonData.scoreboard.status === \"Postponed\") || (jsonData.scoreboard.status === \"Suspended\")) {\n            jQuery(div_id + \" .event_current_status\").html(\"POSTPONED\");\n        }\n        if (jsonData.scoreboard.status === \"Cancelled\") {\n            jQuery(div_id + \" .event_current_status\").html(\"CANCELLED\");\n        }\n        if (jsonData.scoreboard.status === \"Delayed\") {\n            jQuery(div_id + \" .event_current_status\").html(\"DELAYED\");\n        }\n        if ((jsonData.scoreboard.status === \"Forfeit by Away\") || (jsonData.scoreboard.status === \"Forfeit by Home\") ||\n            (jsonData.scoreboard.status === \"Forfeit by Away Team - Conference Only\") || (jsonData.scoreboard.status === \"Vacated by Away Team\") ||\n            (jsonData.scoreboard.status === \"Vacated by Home Team\")) {\n            jQuery(div_id + \" .event_current_status\").html(\"FORFEIT\");\n        }\n        if ((jsonData.scoreboard.status === \"Pre-Game\") || (jsonData.scoreboard.status === \"In-Progress\")) {\n            jQuery(div_id + \" .event_status .event_current_status\").append(game_time);\n            jQuery(div_id + \" .event_status .network_winner\").append(jsonData.scoreboard.network);\n        }\n        if (jsonData.scoreboard.data_confirmed !== true) {\n            refreshArray[div_id.replace(\"-\", \"\").replace(\"#\", \"\") + 'scoreboardRefresh'] = setInterval(function() {\n                refreshScoreboard(div_id, league, game_id)\n            }, 60000);\n        } else {\n            //console.log(\"Not Refreshing!\")\n        }\n    });\n}\n\n// --- refresh scoreboard ---\nrefreshScoreboard = function(div_id, league, game_id) {\n    jQuery.ajax({\n        url: \"http://scoreboard\" + scoreboardENV + \".advance.net/\" + league + \"/?gid=\" + game_id,\n        dataType: 'jsonp',\n        cache: true,\n        jsonpCallback: div_id.replace(\"#\", \"\").replace(\"-\", \"\")\n    }).done(function(jsonData) {\n\n        if (league === 'mls'){\n            mlsGoals(jsonData, div_id);\n        }\n\n        var period = sc_period(league, jsonData.scoreboard);\n        if (jsonData.scoreboard.status === \"In-Progress\") {\n            var game_time = jsonData.scoreboard.time;\n        } else {\n            var game_start = new Date(jsonData.scoreboard.date),\n                game_hours = game_start.getHours(),\n                game_minutes = game_start.getMinutes();\n            if (game_hours >= 12) {\n                var am_pm = \"PM\";\n                if (game_hours > 12) {\n                    game_hours = game_hours - 12;\n                }\n            } else {\n                am_pm = \"AM\";\n                if (game_hours === 0) {\n                    game_hours = 12;\n                }\n            }\n            if (game_minutes < 10) {\n                game_minutes = \"0\" + game_minutes;\n            }\n            if (game_minutes === \"00\") {\n                game_minutes = am_pm;\n            } else {\n                game_minutes = \":\" + game_minutes + am_pm;\n            }\n            game_time = game_hours + game_minutes;\n        }\n        if (jsonData.scoreboard.status === \"In-Progress\") {\n            if (jQuery(div_id + \" .team_score\").length === 0) {\n                jQuery(\".team_record\").remove();\n                jQuery(div_id + \" .team_one\").append('<div class=\"team_score\"></div>');\n                jQuery(div_id + \" .team_two\").append('<div class=\"team_score\"></div>');\n            }\n            jQuery(div_id + \" .team_one .team_score\").html(jsonData.scoreboard.teams.away.score);\n            jQuery(div_id + \" .team_two .team_score\").html(jsonData.scoreboard.teams.home.score);\n            jQuery(div_id + \" .event_status .game_date\").html(period);\n            jQuery(div_id + \" .event_current_status\").html(game_time);\n        }\n\n        if (jsonData.scoreboard.status === \"Final\") {\n            jQuery(div_id + \" .event_current_status\").html(\"FINAL\");\n            jQuery(div_id + \" .network_winner\").html(\"\");\n            jQuery(div_id + \" .event_status .game_date\").html((game_start.getMonth() + 1) + \"/\" + game_start.getDate());\n        }\n\n        if (jsonData.scoreboard.data_confirmed === true) {\n            jQuery(div_id + \" .team_one .team_record\").text(calc_record(jsonData.scoreboard.teams.away));\n            jQuery(div_id + \" .team_two .team_record\").text(calc_record(jsonData.scoreboard.teams.home));\n            jQuery(div_id + \" .team_one .team_score\").html(jsonData.scoreboard.teams.away.score);\n            jQuery(div_id + \" .team_two .team_score\").html(jsonData.scoreboard.teams.home.score);\n            if (jsonData.scoreboard.teams.away.score > jsonData.scoreboard.teams.home.score) {\n                jQuery(div_id + \" .team_two .team_score, \" + div_id + \" .team_two .team_name\").addClass(\"lost\");\n            } else if (jsonData.scoreboard.teams.away.score < jsonData.scoreboard.teams.away.score) {\n                jQuery(div_id + \" .team_one .team_score, \" + div_id + \" .team_one .team_name\").addClass(\"lost\");\n            }\n            window.clearInterval(refreshArray[div_id.replace(\"-\", \"\").replace(\"#\", \"\") + 'scoreboardRefresh']);\n        }\n\n        if ((jsonData.scoreboard.status === \"Postponed\") || (jsonData.scoreboard.status === \"Suspended\")) {\n            jQuery(div_id + \" .event_current_status\").html(\"POSTPONED\");\n            jQuery(div_id + \" .network_winner\").html(\"\");\n        }\n\n        if (jsonData.scor