n\n//------------------------------------------------------------------------------------\n/*\n*  Teams Pages\n*/\n// --- close the twitter object\njQuery(document).on('click', '.lf-sm-close', function() {\n    jQuery('#adi-ssm-content').hide();\n});\n\n// --- toggle 'preview comments' link on\njQuery(document).on('click', '.preview-comments.lf-btn', function() {\n    jQuery(this).addClass('active');\n});\n\n// --- toggle 'preview comments' off\njQuery(document).on('click', '.hide-comments.lf-btn', function() {\n    jQuery(this).closest('.river-item').find('.preview-comments.lf-btn').removeClass('active');\n});\n\n\n//------------------------------------------------------------------------------------\n/**\n * Scoreboards\n */\nvar refreshArray = [];\n\nvar scoreboardENV = function(){\n    var _l;\n    // return \"-uat\" or \"-dev\" but ignore if \"origin\" is part of the substring (use prod ajax endpoint)\n    var non_origin = ((_l = window.location.host.split('.')[0].split('-')).length > 1)? '-'+_l[1] : '';\n    if (window.location.host.indexOf(\"local\") > -1)\n        non_origin = '-uat';\n    return (window.location.host.split('.')[0].indexOf('origin') != -1) ? \"\" : non_origin;\n}();\n\n// --- define number of periods in game by league --\nvar sc_period = function(league, scoreboard){\n    var quarters;\n    if (league === 'cbk') {\n        quarters = [\"\", \"1st\", \"2nd\", \"OT\"];\n    } else if (league === 'mlb') {\n        //gross. rip this out immediately.\n        quarters = [\"\", \"1st\", \"2nd\", \"3rd\", \"4th\", \"5th\", \"6th\", \"7th\", \"8th\", \"9th\", \"10th\", \"11th\", \"12th\", \"13th\",\n        \"14th\", \"15th\", \"16th\", \"17th\", \"18th\", \"19th\", \"20th\", \"21st\", \"22nd\"];\n    } else {\n        quarters = [\"\", \"1st\", \"2nd\", \"3rd\", \"4th\", \"OT\"];\n    }\n    var period = quarters[scoreboard.period];\n    return (league != \"mlb\") ? period : (scoreboard.inningDivision + \" \" + period);\n};\n\n// --- build team record ----\n// ( wins - losses), if ties ( wins - losses - ties), if overtime losses ( wins - losses - overtime losses), if ties && overtime losses ( wins - losses - ties - overtime losses )\nvar calc_record = function(rec) {\n    return '(' + rec.wins + '-' + rec.losses + ((rec.ties > 0) ? '-' + rec.ties : '')+ ((rec.overtimeLosses > 0) ? '-' + rec.overtimeLosses : '') + ')';\n};\n\n// --- grab team name or location\nvar addTeamNames = function(div_id, league, teams) {\n    var teamInfo = [{ placement: '.team_one',location: 'away' }, { placement: '.team_two', location: 'home' }];\n    for (var i = 0; i < teamInfo.length; i++ ) {\n        jQuery(div_id + \" \" + teamInfo[i].placement + \" .team_name\").prepend((league === \"cfb\" || league === \"cbk\" || league === \"wcbk\" ||  teams[teamInfo[i].location].name === \"\") ? teams[teamInfo[i].location].location : teams[teamInfo[i].location].name);\n    }\n};\n\n// --- MLS Goals ---\nvar mlsGoals = function(jsonData, div_id) {\n    var _periods = jsonData.scoreboard.periodDetails;\n    var _goalData = {};\n\n    // --- name keys after team abbreviation ---\n    _goalData[(jsonData.scoreboard.teams.home.short_name).toUpperCase()] = { goals: \"\", location: '.team_two'};\n    _goalData[(jsonData.scoreboard.teams.away.short_name).toUpperCase()] = { goals: \"\", location: '.team_one'};\n\n    // --- hide team_record, and place into team_name data --\n    if (jsonData.scoreboard.status === \"In-Progress\" || jsonData.scoreboard.status === \"Final\") {\n        jQuery(div_id).find('.gda_scores').addClass('mls-format');\n    }\n    jQuery(div_id).find('.team_one .team_name').attr('data-record', calc_record(jsonData.scoreboard.teams.away));\n    jQuery(div_id).find('.team_two .team_name').attr('data-record', calc_record(jsonData.scoreboard.teams.home));\n\n    // --- grab goals scored ---\n    if (Object.keys(_periods)) {\n        for (var p in _periods) {\n            if (p != undefined) {\n                for ( var g in _periods[p].goals) {\n                    var _goal =  _periods[p].goals[g];\n                    var _playerName = _goal.player.displayName.split(' ');\n                    _playerName = _playerName[(_playerName.length - 1)];\n                    _goalData[(_goal.team.abbreviation).toUpperCase()].goals += '<span class=\"player-block\">' + _playerName + ' <strong>' + _goal.time.minutes + '\\'</strong></span> ';\n                }\n            }\n        }\n    }\n\n    // --- add elem if missing, otherwise fill ---\n    for (var s in _goalData) {\n        if (!jQuery(div_id).find(_goalData[s].location + ' .mls-goals-scored').length) {\n            jQuery(div_id).find(_goalData[s].location).append('<div class=\"mls-goals-scored\">' + _goalData[s].goals + '</div>');\n        } else {\n            jQuery(div_id).find(_goalData[s].location + ' .mls-goals-scored').html( _goalData[s].goals);\n        }\n    }\n};\n\n// --- build scoreboard ---\nfunction buildScoreboard(div_id, league, game_id, cta_text, cta_url) {\n    jQuery.ajax({\n        url: \"http://scoreboard\" + scoreboardENV + \".advance.net/\" + league + \"/?gid=\" + game_id,\n        dataType: 'jsonp',\n        cache: true,\n        jsonpCallback: div_id.replace(\"#\", \"\").replace(\"-\", \"\")\n    }).done(function(jsonData) {\n\n        // --- on successful ajax build stuff ---\n        // console.log('game data:', jsonData);\n\n        // --- grab period number ---\n        var period = sc_period(league, jsonData.scoreboard);\n\n        // --- check if game is in progress ---\n        if (jsonData.scoreboard.status === \"In-Progress\") {\n            var game_time = jsonData.scoreboard.time;\n        } else {\n            var game_start = new Date(jsonData.scoreboard.date),\n                game_hours = game_start.getHours(),\n                game_minutes = game_start.getMinutes();\n            if (game_hours >= 12) {\n                var am_pm = \"PM\";\n                if (game_hours > 12) {\n                    game_hours = game_hours - 12;\n                }\n            } else {\n                am_pm = \"AM\";\n                if (game_hours === 0) {\n                    game_hours = 12;\n                }\n            }\n            if (game_minutes < 10) {\n                game_minutes = \"0\" + game_minutes;\n            }\n            if (game_minutes === \"00\") {\n                game_minutes = am_pm;\n            } else {\n                game_minutes = \":\" + game_minutes + am_pm;\n            }\n            game_time = game_hours + game_minutes;\n        }\n\n        addTeamNames(div_id, league, jsonData.scoreboard.teams);\n\n        if ((cta_text != \"\") && (cta_url != \"\")) {\n            AdManager.injectUnit(['Button1','Button1']);\n            jQuery(div_id + \" .join_discussion\").append(cta_text);\n            jQuery(div_id + \" .join_discussion\").attr(\"href\", cta_url);\n            jQuery(div_id + \" .join_discussion\").show();\n        } else {\n            jQuery(div_id + ' .gda_discussion_ad').remove();\n            jQuery(div_id).find('.gda_scores').css({\n                'margin': '0 auto'\n            });\n        }\n\n        /* show|hide team record */\n        var team_record_home = (jQuery.isNumeric(jsonData.scoreboard.teams.home.wins) && jQuery.isNumeric(jsonData.scoreboard.teams.home.losses)) ? true : false;\n        var team_record_away = (jQuery.isNumeric(jsonData.scoreboard.teams.away.wins) && jQuery.isNumeric(jsonData.scoreboard.teams.away.losses)) ? true : false;\n        if ((jsonData.scoreboard.status == \"Pre-Game\") || ((jsonData.scoreboard.status === \"Delayed\") && (jQuery.isNumeric(jsonData.scoreboard.teams.home.score) === false) && (jQuery.isNumeric(jsonData.scoreboard.teams.away.score) === false)) || ((jsonData.scoreboard.status === \"Postponed\") && (jQuery.isNumeric(jsonData.scoreboard.teams.home.score) === false) && (jQuery.isNumeric(jsonData.scoreboard.teams.away.score) === false)) || ((jsonData.scoreboard.status === 