            "text": "(function( window, undefined ){\r\n\r\n    'use strict';\r\n\r\nvar gg = window.gg = window.gg || {};\r\nvar md = window.md = window.md || {};\r\nvar lv = window.lv = window.lv || {};\r\nvar clublv = window.clublv = window.clublv || {};\r\nvar muyfan = window.muyfan = window.muyfan || {};\r\nvar cartelera = window.cartelera = window.cartelera || {};\r\nvar msg = window.msg = window.msg || {};\r\nvar rac1 = window.rac1 = window.rac1 || {};\r\nvar myaccount = window.myaccount = window.myaccount || {};\ngg.AccountsService = function() {\r\n\r\n    var regToken = null;\r\n    var loginProvider = null;\r\n    \r\n    var RESPONSE_CODES = {\r\n        'SUCCESS': 0,\r\n        'ACCOUNTS_LINKED': 200009,\r\n        'EMAIL_CONFLICT': {\r\n            'ERROR': 403043,\r\n            'WARN': 200010\r\n        },\r\n        'IDENTIFIER_CONFLICT': 400003,\r\n        'PENDING_REGISTRATION': 206001,\r\n        'INVALID_LOGIN': 403042,\r\n        'VALIDATION_ERRORS': 400009\r\n    };\r\n\r\n    var safeCallback = function(callback, data) {\r\n        if (callback) {\r\n            callback(data);\r\n        }\r\n    };\r\n\r\n    /**\r\n     * Appends to data the conflicting account information needed to resolve the\r\n     * login identifier conflict and executes the callback\r\n     */\r\n    var addAccountConflict = function(data, callback) {\r\n        gigya.accounts.getConflictingAccount({\r\n            regToken: regToken,\r\n            format: 'jsonp',\r\n            callback: function(resp) {\r\n                data.accountConflict = resp.conflictingAccount;\r\n                safeCallback(callback, data);\r\n            }\r\n        });\r\n    };\r\n\r\n    var login = function(email, password, callback) {\r\n        gigya.accounts.login({\r\n            loginID: email,\r\n            password: password,\r\n            format: \"jsonp\",\r\n            callback: function(resp) {\r\n                regToken = resp.regToken;\r\n                loginProvider = \"site\";\r\n                safeCallback(callback, resp);\r\n            }\r\n        });\r\n    };\r\n\r\n    var socialLogin = function(provider, callback) {\r\n        gigya.accounts.socialLogin({\r\n            provider: provider,\r\n            callback: function(resp) {\r\n                regToken = resp.regToken;\r\n                loginProvider = provider;\r\n                if (resp.errorCode == RESPONSE_CODES.EMAIL_CONFLICT.ERROR) {\r\n                    addAccountConflict(resp, callback);\r\n                } else {\r\n                    safeCallback(callback, resp);\r\n                }\r\n            }\r\n        });\r\n    };\r\n\r\n    var signup = function(email, password, username, data, callback) {\r\n        gigya.accounts.initRegistration({\r\n            callback: function(initResp) {\r\n                gigya.accounts.register({\r\n                    email: email,\r\n                    password: password,\r\n                    username: username,\r\n                    regToken: initResp.regToken,\r\n                    data: data,\r\n                    profile: {\r\n                        email: email\r\n                    },\r\n                    finalizeRegistration: true,\r\n                    callback: function(resp) {\r\n                        regToken = resp.regToken;\r\n                        loginProvider = 'site';\r\n                        safeCallback(callback, resp);\r\n                    }\r\n                });\r\n            }\r\n        });\r\n    };\r\n\r\n    /**\r\n     * Completes a user registration with the required data\r\n     */\r\n    var completeSignup = function(email, data, callback) {\r\n        gigya.accounts.setAccountInfo({\r\n            regToken: regToken,\r\n            profile: {\r\n                email: email\r\n            },\r\n            data: data,\r\n            conflictHandling: 'saveProfileAndFail',\r\n            callback: function(resp) {\r\n                if (resp.errorCode == RESPONSE_CODES.SUCCESS) {\r\n                    // Request registration finalization\r\n                    gigya.accounts.finalizeRegistration({\r\n                        regToken: regToken,\r\n                        callback: function(resp) {\r\n                            safeCallback(callback, resp);\r\n                        }\r\n                    });\r\n\r\n                } else if (resp.errorCode == RESPONSE_CODES.EMAIL_CONFLICT.WARN) {\r\n                    addAccountConflict(resp, callback);\r\n                }\r\n            }\r\n        });\r\n    };\r\n\r\n    /**\r\n     * Links a social account to it's email matching site account\r\n     */\r\n    var linkAccount = function(email, password, callback) {\r\n        gigya.accounts.login({\r\n            loginID: email,\r\n            password: password,\r\n            loginMode: 'link',\r\n            regToken: regToken,\r\n            format: \"jsonp\",\r\n            callback: function(resp) {\r\n                // Login in success\r\n                if (resp.errorCode == RESPONSE_CODES.ACCOUNTS_LINKED) {\r\n                    login(email, password, callback);\r\n                } else {\r\n                    safeCallback(callback, resp);\r\n                }\r\n            }\r\n        });\r\n    };\r\n\r\n    /**\r\n     * Links an account to a given social account\r\n     */\r\n    var socialLinkAccount = function(linkProvider, callback) {\r\n        gigya.accounts.socialLogin({\r\n            provider: linkProvider,\r\n            loginMode: 'link',\r\n            regToken: regToken,\r\n            format: \"jsonp\",\r\n            callback: function(resp) {\r\n                // Login in success\r\n                if (resp.errorCode == RESPONSE_CODES.ACCOUNTS_LINKED) {\r\n                    var provider = loginProvider && loginProvider != 'site' ? loginProvider : linkProvider;\r\n                    socialLogin(provider, callback);\r\n                } else {\r\n                    safeCallback(callback, resp);\r\n                }\r\n            }\r\n        });\r\n    };\r\n\r\n    var resetPassword = function(email, callback) {\r\n        gigya.accounts.resetPassword({\r\n            loginID: email,\r\n            format: \"jsonp\",\r\n            callback: function(resp) {\r\n                safeCallback(callback, resp);\r\n            }\r\n        });\r\n    };\r\n\r\n    var resendVerificationEmail = function(email) {\r\n        gigya.accounts.resendVerificationCode({\r\n            regToken: regToken,\r\n            email: email\r\n        });\r\n    };\r\n    \r\n    var updateUser = function(profile, data, callback) {\r\n        var info = {\r\n            data: data,\r\n            callback: function(resp) {\r\n                safeCallback(callback, resp);\r\n            }\r\n        };\r\n        if(profile){\r\n            profile.email = gg.accounts.getUser().email; //block email changes\r\n            info.profile = profile;\r\n            if(profile.username){\r\n                info.username = profile.username;\r\n            }\r\n        }\r\n        gigya.accounts.setAccountInfo(info);\r\n    };\r\n\r\n    var logout = function() {\r\n        gigya.accounts.logout();\r\n        regToken = null;\r\n    };\r\n\r\n    return {\r\n        RESPONSE_CODES: RESPONSE_CODES,\r\n        login: login,\r\n        socialLogin: socialLogin,\r\n        signup: signup,\r\n        completeSignup: completeSignup,\r\n        linkAccount: linkAccount,\r\n        socialLinkAccount: socialLinkAccount,\r\n        resetPassword: resetPassword,\r\n        resendVerificationEmail: resendVerificationEmail,\r\n        updateUser: updateUser,\r\n        logout: logout,\r\n        loginProvider: function(){ return loginProvider; }\r\n    };\r\n};\n/**\r\n * @class EventRegistry - Register functions to be executed when an event is\r\n *        triggered. Order of registration and trigger is not important.\r\n */\r\n// @example\r\n// var registry = new gg.EventRegistry();\r\n// registry.on('xxx', function(args) {\r\n// ____console.log('xxx was triggered with args', args);\r\n// });\r\n// registry.trigger('xxx', ['a', 1]);\r\ngg.EventRegistry = function() {\r\n\r\n    var events = {};\r\n    var handlers = {};\r\n\r\n    var on = function(event, handler) {\r\n        if (!handlers[event]) {\r\n            handlers[event] = [];\r\n        }\r\n        handlers[event].push(handler);\r\n        if (events[event] != undefined) {\r\n            handler(events[event]);\r\n        }\r\n    };\r\n\r\n    var trigger = function(event, args) {\r\n        events[event] = args || [];\r\n        gg.util.map(handlers[event] || [], function(handler) {\r\n            try {\r\n                handler(args);\r\n            } catch (e) {\r\n                console.error('Error occurred for handler of event {}', event, handler, args, e);\r\n            }\r\n        });\r\n    };\r\n\r\n    return {\r\n        on: on,\r\n        trigger: trigger\r\n    };\r\n};\r\n\ngg.WindowLoad = function(options) {\r\n\r\n    var onCloseAction = null;\r\n    var isEmbedded = options && options.selector;\r\n\r\n    var element = $('<div></div>');\r\n    if (isEmbedded) {\r\n        element = $(options.selector);\r\n    } else {\r\n        $('body').append(element);\r\n    }\r\n\r\n    var load = function(url, data, options) {\r\n        var el = element;\r\n        if (options && options.selector) {\r\n            el = el.find(options.selector);\r\n        }\r\n        el.load(url, data);\r\n    };\r\n\r\n    var close = function() {\r\n        if (isEmbedded) {\r\n            element.empty();\r\n        } else {\r\n            element.remove();\r\n        }\r\n        if (onCloseAction) {\r\n            onCloseAction();\r\n        }\r\n    };\r\n\r\n    var onClose = function(action) {\r\n        onCloseAction = action;\r\n    };\r\n\r\n    return {\r\n        load: load,\r\n        close: close,\r\n        onClose: onClose\r\n    };\r\n};\r\n\n/**\r\n * TODO: Make possible to use this from an iframe\r\n */\r\ngg.accounts = (function() {\r\n\r\n    var loaded = false;\r\n    var data = undefined;\r\n    var options = null;\r\n    var screens = [];\r\n    var service = new gg.AccountsService();\r\n    var dependencies = [];\r\n    var dependenciesInitialized = false;\r\n\r\n    var SCREEN_URLS = {\r\n        \"tes\": \"https://my-account.grupogodo.biz/screens\",\r\n        \"pro\": \"https://my-account.grupogodo.com/screens\"\r\n    };\r\n\r\n    var APIKEYS = {\r\n        \"tes\": {\r\n            \"GG\": \"3_6udun_Rw447kxXnAHHbhiDfYTKwfFMflyRn5rB02S7YIYPhFffMHRf53SKyXKyAQ\",\r\n            \"LV\": \"3_0N1zt-3LhS7fEmF8WFmBIgKi2D9EQfNkmz2oVvg-zQ_zQTrJzkh0LTjzs8FIg2Xl\",\r\n            \"MD\": \"3_Z9AaGhNCKHrv_arJAYTjLW8Wxoj5EUGVJWI4_yoYq-zifCvnqvrYCMXbHawqDIAM\",\r\n            \"CLUB\": \"3_QIvPrasAlrfImPN8VWsCBDdkFKRoCAIf1cYe5nWI5W2RCg5BhmqeCdF3rGGaqk2c\",\r\n            \"SHOPPING_LV\": \"3_8KpHVyd6IXifXXvescs8gyszheViiHUnlU2HQZQ1yQb3m904c5yyGRLRlX2x7Oie\",\r\n            \"SHOPPING_MD\": \"3_eV3Jnk5AFKJzOWQ7RDrfVfferYGoybwpandnBpz8vwdnt3p0u8ctKOKWYhm4zh0f\",\r\n            \"R1\": \"3_cTiILSAkQhDYYp6eNK8AnOb17oVBiOi22lTSEiTKtX12ht_tgDYR3q7mGgM-8z6J\",\r\n            \"FORMACION_LV\": \"3_qkycjeiPLd7B3TfCW5CvqN3HdSOrbB4zND_-mw68y_KaIB22E2V8oOPCaP0pXSzb\"\r\n        },\r\n        \"pro\": {\r\n            \"GG\": \"3_68tV4HI6WJ-PjvLN3XXeZ2uIqqUbWYP4lzHgjClpfXA9BKrfLziK2vVLnkhZpMZr\",\r\n            \"LV\": \"3_EU3-xEDkFUSjKIeCcZWBtlFmA3Zzm5MM3NgG0wU7wZT-6OHafHUFSJcO-qoj7Ig_\",\r\n            \"MD\": \"3_aAT7HdWFf7gKgmUhveckx7SagFSKnV9uFTB1LtO1-XS_jABEmMZ9IfgDMyBezUbL\",\r\n            \"CLUB\": \"3_LGv6WZV5Wf9S_iVEnhKYRzWmB6PIfc1PKzjdvIwVLcBpnu4JjblBbaHBnFWTUsmB\",\r\n            \"SHOPPING_LV\": \"3_1g-FAC2pFUruAiCBVzcZpTP4nYAz8oPB5Aww6imSffUuka4XdIAuotCqniaNo5w3\",\r\n            \"SHOPPING_MD\": \"3_ufFYkB6tOhTmmMBAOHgql2-ikBSmcnM-RUU8s3pZ5lZbAKEsLfWR5Nmo0H85MDv8\",\r\n            \"R1\": \"3_8vf5faEXxQTIpqw9u1e3P8dniAeHshnwHEoUXedwd-4hFiMt_Mi_vMmXI7uzzdCX\",\r\n            \"FORMACION_LV\": \"3_4ATxhEyAcsyPwQr6XbLOante8yZEEJcnoRhRxWArxqd2xscLHfT130GUMHLUw3MI\"\r\n        }\r\n    };\r\n\r\n    /**\r\n     * List of actions to perform when is detected that the user is logged or\r\n     * not\r\n     */\r\n    var registry = new gg.EventRegistry();\r\n\r\n    /**\r\n     * Downloads Gigya script and initializes it with common configuration.\r\n     * After load, refresh method is called. Keeps \"GigyaId\" in a cookie, too.\r\n     */\r\n    var init = function(site, opts) {\r\n        options = opts ? opts : {};\r\n        options.site = (opts && opts.site) ? opts.site : site;\r\n        options.env = (opts && opts.env) ? opts.env : \"pro\";\r\n        options.url = (opts && opts.url) ? opts.url : SCREEN_URLS[options.env];\r\n\r\n        if (!options.excludeAccountsManagement) { //Temp until accounts flow released in MD\r\n            if (loaded) {\r\n                refresh();\r\n\r\n            } else {\r\n                gg.util.getScript('https://cdns.gigya.com/js/gigya.js?apiKey=' + APIKEYS[options.env][site.toUpperCase()], {\r\n\r\n                    content: JSON ? JSON.stringify({\r\n                        defaultMobileRegScreenSet: null,\r\n                        defaultRegScreenSet: null,\r\n                        lang: 'es',\r\n                        connectWithoutLoginBehavior: 'alwaysLogin',\r\n                        sessionExpiration: 30 * 24 * 60 * 60\r\n                    }) : '',\r\n\r\n                    callback: function() {\r\n                        loaded = true;\r\n                        gigya.accounts.addEventHandlers({\r\n                            onLogout: refresh\r\n                        });\r\n                        refresh();\r\n                    }\r\n                });\r\n            }\r\n        }\r\n    };\r\n\r\n    var register = function(id, f, args) {\r\n        dependencies.push({\r\n            id: id,\r\n            f: f,\r\n            args: args\r\n        });\r\n        if (dependenciesInitialized) {\r\n            gg.registry.register(id, f, args);\r\n        }\r\n    };\r\n\r\n    var initDependencies = function() {\r\n        gg.util.map(dependencies, function(dependency) {\r\n            gg.registry.register(dependency.id, dependency.f, dependency.args);\r\n        });\r\n    };\r\n\r\n    /**\r\n     * Sets current user's data or null if it's not logged in. Triggers\r\n     * execution of registered functions.\r\n     */\r\n    var refresh = function() {\r\n        gigya.accounts.getAccountInfo({\r\n            include: 'loginIDs,profile,data',\r\n            extraProfileFields: 'username',\r\n            callback: function(resp) {\r\n                var stateChanged = false;\r\n\r\n                if (resp.errorCode == 0) { // user is logged\r\n                    data = resp;\r\n                    // evaluate if there's been a login or an autologin\r\n                    if (!gg.cookie.get(gg.accounts.cookieNameUserId)) {\r\n                        gg.cookie.set(gg.accounts.cookieNameUserId, getUser().UID);\r\n                        stateChanged = true;\r\n                    }\r\n                    // refresh username in case it's been changed\r\n                    gg.cookie.set(gg.accounts.cookieNameUsername, getUser().username);\r\n                    trigger('logged', stateChanged);\r\n\r\n                } else {\r\n                    data = null;\r\n                    // evaluate if there's been a logout or an autologout\r\n                    if (gg.cookie.get(gg.accounts.cookieNameUserId)) {\r\n                        gg.cookie.remove(gg.accounts.cookieNameUserId);\r\n                        gg.cookie.remove(gg.accounts.cookieNameUsername);\r\n                        stateChanged = true;\r\n                    }\r\n                    trigger('unlogged', stateChanged);\r\n                }\r\n\r\n                if (!dependenciesInitialized) {\r\n                    // initialize dependencies after user state has been\r\n                    // evaluated\r\n                    initDependencies();\r\n                    dependenciesInitialized = true;\r\n                }\r\n            }\r\n        });\r\n    };\r\n\r\n    /**\r\n     * Registers an action to perform when an event (logged/unlogged detection)\r\n     * occurs. If execute parameter is true, action is also executed at the\r\n     * moment.\r\n     */\r\n    var on = function(action, event) {\r\n        registry.on(event, action);\r\n    };\r\n\r\n    var onLogged = function(action) {\r\n        on(action, 'logged');\r\n    };\r\n\r\n    var onUnlogged = function(action) {\r\n        on(action, 'unlogged');\r\n    };\r\n\r\n    var isLogged = function() {\r\n        return data ? true : false;\r\n    };\r\n\r\n    /**\r\n     * Returns current user filtered info or null if it's not logged in.\r\n     */\r\n    var getUser = function(extra) {\r\n        if (data == null) {\r\n            return null;\r\n        }\r\n\r\n        var user = {\r\n            UID: data.UID,\r\n            firstName: data.profile.firstName,\r\n            lastName: data.profile.lastName,\r\n            email: data.profile.email,\r\n            gender: data.profile.gender,\r\n            nickname: data.profile.nickname,\r\n            username: data.profile.username,\r\n            age: data.profile.age,\r\n            thumbnail: data.profile.thumbnailURL,\r\n            isVerified: data.isVerified,\r\n            loginProvider: data.loginProvider\r\n        };\r\n\r\n        if (extra) {\r\n            user.authToken = {\r\n                uid: data.UID,\r\n                signature: data.UIDSignature,\r\n                timestamp: data.signatureTimestamp\r\n            };\r\n            user.profile = data.profile;\r\n            user.data = data.data;\r\n            user.creationDate = data.created;\r\n        }\r\n        return user;\r\n    };\r\n\r\n    var trigger = function(event, args) {\r\n        registry.trigger(event, args);\r\n    };\r\n\r\n    var login = function(data) {\r\n        show('login', data);\r\n    };\r\n\r\n    var getScreen = function(id) {\r\n        return screens[id];\r\n    };\r\n\r\n    var show = function(screenName, data, opts) {\r\n\r\n        var params = $.extend({}, options, data);\r\n        params.screen = screenName;\r\n        params.screenId = screens.length;\r\n        params.layout = (opts && opts.selector) ? 'embedded' : 'popup';\r\n\r\n        var screen = new gg.WindowLoad(opts);\r\n        screens.push(screen);\r\n        screen.onClose(function() {\r\n            refresh();\r\n        });\r\n\r\n        screen.load(params.url, params);\r\n    };\r\n\r\n    var logout = function() {\r\n        service.logout();\r\n    };\r\n\r\n    return {\r\n        init: init,\r\n        onLogged: onLogged,\r\n        onUnlogged: onUnlogged,\r\n        isLogged: isLogged,\r\n        getUser: getUser,\r\n        show: show,\r\n        login: login,\r\n        logout: logout,\r\n        cookieNameUserId: 'gigyaId',\r\n        cookieNameUsername: 'gigyaUsername',\r\n        getScreen: getScreen,\r\n        service: service,\r\n        register: register\r\n    };\r\n\r\n}());\n// TODO: Repensar el librariesToLoad\r\n\r\n/**\r\n * TODO: header bidding:\r\n * <ul>\r\n * <li>ha de funcionar només a /internacional i .biz: haurem de passar una kv</li>\r\n * </ul>\r\n */\r\ngg.ads = (function() {\r\n\r\n    /**\r\n     * @property Whole configuration object passed to load function\r\n     */\r\n    var cfg = {};\r\n\r\n    /**\r\n     * @property Map containing current sequence for each 'sequential' type\r\n     */\r\n    var sequences = {};\r\n\r\n    /**\r\n     * @property Number of libraries that must be loaded and processed before\r\n     *           requesting DFP script\r\n     */\r\n    var librariesToLoad = 0;\r\n\r\n    /**\r\n     * @property Name of the slots disabled (won't be defined or displayed) for\r\n     *           current breakpoint\r\n     */\r\n    var disabledSlots = {};\r\n\r\n    /**\r\n     * @property Name of the slots that would be filled with Criteo whitelisted\r\n     *           ads when AdBlock Plus is detected, for current breakpoint\r\n     */\r\n    var abpSlots = {};\r\n\r\n    /**\r\n     * @property Object containing all slots for current breakpoint\r\n     */\r\n    var slots = {};\r\n\r\n    /**\r\n     * @property Registry for callbacks after rendering slots\r\n     */\r\n    var events = new gg.EventRegistry();\r\n\r\n    /**\r\n     * @property Randomly selected \"ad_group\" value\r\n     */\r\n    var adomikGroup;\r\n\r\n    /**\r\n     * @property Prebid configuration for current breakpoint\r\n     */\r\n    var prebidCfg;\r\n\r\n    /**\r\n     * @method Do some startup and loads all required libraries (concurrently)\r\n     *         before loading DFP\r\n     * \r\n     * @param {object}\r\n     *            [config] - Configuration\r\n     * @param {object}\r\n     *            [config.disabledSlots] - Object containing an array of\r\n     *            disabled slot names for each breakpoint\r\n     * @param {string[]}\r\n     *            [config.sequential|sectionable] - Array of sequential or\r\n     *            sectionable types\r\n     * @param {string}\r\n     *            [config.unitname] - DFP ad unit path\r\n     * @param {object}\r\n     *            [config.keys] - Key/value pairs for page level targeting\r\n     * @param {boolean}\r\n     *            [config.syncLoading|syncRendering]\r\n     * @param {string}\r\n     *            [config.loadPG|loadCriteo|loadProxistore]\r\n     * @param {bolean}\r\n     *            [config.adomik]\r\n     * @param {object}\r\n     *            [config.prebid] - Header bidding configuration\r\n     * @param {number}\r\n     *            [config.prebid.timeout] - Max miliseconds before loading DFP\r\n     *            if header bidding hasn't finished\r\n     * \r\n     */\r\n    var load = function(config) {\r\n\r\n        cfg = config;\r\n\r\n        var breakpoint = gg.breakpoint.getCurrent();\r\n        cfg.keys.d = breakpoint;\r\n        cfg.keys.adUnitPath = cfg.unitname;\r\n\r\n        // If for whatever reason an @ is on the URL, don't call DFP, as it\r\n        // could lead to a PII violation\r\n        var url = document.location.href;\r\n        if (url.indexOf('@') >= 0 || url.indexOf('%40') >= 0) {\r\n            return;\r\n        }\r\n\r\n        if (cfg.abpSlots && cfg.abpSlots[breakpoint]) {\r\n            abpSlots = cfg.abpSlots[breakpoint];\r\n            loadCriteoAbp();\r\n        }\r\n\r\n        var scenario = gg.util.url().params.sc;\r\n        if (scenario) {\r\n            cfg.keys.sc = scenario;\r\n        }\r\n\r\n        gg.util.map((cfg.disabledSlots[breakpoint] || []).concat(window.moreDisabled || []), function(name) {\r\n            disabledSlots[name] = true;\r\n        });\r\n\r\n        window.googletag = {\r\n            cmd: []\r\n        };\r\n\r\n        gg.util.map([['loadPG', loadPG], ['loadCriteo', loadCriteo], ['loadProxistore', loadProxistore], ['adomik', resolveAdomikGroup]],\r\n                function(lib) {\r\n                    var id = cfg[lib[0]];\r\n                    if (id) {\r\n                        librariesToLoad++;\r\n                        lib[1](id);\r\n                    }\r\n                });\r\n\r\n        var prebidSlots = gg.util.getByName('prebid.conf.' + breakpoint, cfg);\r\n        if (prebidSlots) {\r\n            prebidCfg = {\r\n                slots: prebidSlots,\r\n                timeout: cfg.prebid.timeout\r\n            };\r\n            loadPrebid();\r\n        }\r\n        loadDfp();\r\n    };\r\n\r\n    /**\r\n     * Executes a function, directly if we loaded libraries synchronously, or\r\n     * through command array in case we don't\r\n     */\r\n    var dfpExec = function(f) {\r\n        try {\r\n            if (cfg.syncLoading) {\r\n         