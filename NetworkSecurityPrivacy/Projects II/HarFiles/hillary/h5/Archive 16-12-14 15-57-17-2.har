caption = document.createElement('span');\n    embiggening.elems.caption.className = 'caption';\n\n    embiggening.elems.overlayDiv.appendChild(embiggening.elems.containerDiv);\n    embiggening.elems.containerDiv.appendChild(embiggening.elems.modalDiv);\n    embiggening.elems.modalDiv.appendChild(embiggening.elems.closerDiv);\n    embiggening.elems.modalDiv.appendChild(embiggening.elems.contentDiv);\n    embiggening.elems.contentDiv.appendChild(embiggening.elems.anchor);\n    embiggening.elems.contentDiv.appendChild(embiggening.elems.caption);\n    embiggening.elems.footerElem.appendChild(embiggening.elems.overlayDiv);\n\n    return true;\n};\n\nembiggening.initImages = function(parentElement) {\n    if(embiggening.initEmbiggen === false) {\n        return false;\n    }\n    var elems = parentElement.querySelectorAll('.full-size-popup');\n    var i;\n    for(i=0; i<elems.length; i++) {\n        var elem = elems[i];\n\n        // Diactivate click until after \n        var preloadOverlayDiv = embiggening.createAndAttachPreloadOverlay(elem);\n\n        // Preload images\n        var largeImage = embiggening.preloadImages(elem);\n\n        embiggening.setClickEvent(elem, largeImage);\n    }\n};\n\nembiggening.createAndAttachPreloadOverlay = function(elem) {\n    var preloadOverlayDiv = document.createElement('div');\n    preloadOverlayDiv.className = 'embiggened_preload_overlay';\n    preloadOverlayDiv.style.display = 'none';\n    preloadOverlayDiv.style.position = 'relative';\n    elem.parentElement.appendChild(preloadOverlayDiv);\n    return preloadOverlayDiv;\n};\n\nembiggening.preloadImages = function(containerElem) {\n    var imageURL = containerElem.href;\n    var newImage = new Image();\n    newImage.className = 'embigger';\n    newImage.src = imageURL;\n    return newImage;\n};\n\nembiggening.setClickEvent = function(containerElem, fullImg) {\n    containerElem.addEventListener('click',function(e) {\n        e.preventDefault();\n        e.stopPropagation();\n   