            "text": "// I LIVE ON SHARED-DEV!!!! AND IM NEW!!!!\n/***\n * Movable Type Community Solution Functions\n */\n\n// mt.js vars that never change\nvar mediaURL = \"http://media\" + advEnvPrefix + \".\" + advDomain + \"/design/\" + advVersion; \nvar avatarURL = \"http://media\" + advEnvPrefix + \".\" + advDomain + \"/avatars\"; \nvar mtRelativeCGIPath = \"/cgi-bin/mte/\";\nvar mtCommunityScript = \"community/\";\nvar mtCaptchaFields = \"\";\nvar mtSessionCookieName = \"\"; // Backwards compatibility for old libs\nvar mtCaptchaVisible = \"\";\nvar DEBUG;\n\nif ( window.location.hash.toLowerCase().match(/debug/) )\n    DEBUG = 1;\n\n//// Check for Advance namespace\nif ( !window.Advance ) {\n    window.Advance = {};\n    Advance.MT = {};\n    Advance.MT.auth = {};\n    Advance.MT.util = {};\n}\nelse if ( !Advance.MT ) {\n    Advance.MT = {};\n    Advance.MT.auth = {};\n    Advance.MT.util = {};\n}\nelse if ( !Advance.MT.auth ) {\n    Advance.MT.auth = {};\n}\nelse if ( !Advance.MT.util ) {\n    Advance.MT.util = {};\n}\n\nif ( ! Advance.MT.UGC ) {\n    Advance.MT.UGC = {};\n    Advance.MT.UGC.Photo = {};\n    Advance.MT.UGC.Video = {};\n    Advance.MT.UGC.Post = {};\n}\n\nAdvance.MT.init = function() {\n    if ( DEBUG && !jQuery.browser.msie ) \n        console.debug(arguments.callee, arguments);\n\n    // quit if this function has already been called\n    if (arguments.callee.done) return;\n\n    // flag this function so we don't do the same thing twice\n    arguments.callee.done = true;\n\n    // kill the timer\n    // DWD - check against window\n    if ( window._timer ) clearInterval(window._timer);\n\n    // DWD - fire the window onload now, and replace it\n    if ( window.onload && ( window.onload !== window.mtInit ) ) {\n        window.onload();\n        window.onload = function() {};\n    }\n\n    // 'Subscribe' to Gigya events via Social's framework\n    window.Advance.Gigya.fn.subscribe('onLogin.adi', Advance.MT.auth.updateDynamicForms);\n    window.Advance.Gigya.fn.subscribe('onLogout.adi', Advance.MT.auth.updateDynamicForms);\n    window.Advance.Gigya.fn.subscribe('getAccountInfo.adi', Advance.MT.auth.updateDynamicForms);\n\n    Advance.MT.auth.updateDynamicForms();\n};\n\n// Update content forms that require authentication\n// We're using Social's Gigya library to query the user's session status\nAdvance.MT.auth.updateDynamicForms = function() {\n    if ( DEBUG && !jQuery.browser.msie ) \n        console.debug(arguments.callee, arguments);\n\n    try {\n        if ( window.Advance.Gigya.auth.isLogged() ) {\n            mtShow('comments-form');\n            mtHide('SignInForm');\n            if (mtCaptchaVisible)\n                mtHide('comments-open-captcha');\n\n            if ( typeof UGCType === 'undefined' )\n                UGCType = '';\n\n            if ( UGCType === 'photo' )\n                Advance.MT.UGC.Photo.initUploadForm();\n            else if ( UGCType === 'video' )\n                Advance.MT.UGC.Video.initUploadForm();\n            else if ( UGCType === 'post' )\n                Advance.MT.UGC.Post.initEntryForm();\n            else \n                console.log('Cannot determine UGC content, form unrendered');\n        } \n        else {\n            mtHide('comments-form');\n            mtShow('SignInForm');\n        }\n    } catch (e) {\n        console.log( \"Error when updating dynamic forms, cannot validate Gigya user: \", e );\n\n        mtHide('comments-form');\n        mtShow('SignInForm');\n\n        return false;\n    }\n\n    // 'sign in' renders the toprail gigya dropdown\n    jQuery('a#sign-in-link').on('click', function(){jQuery('#adv_account').trigger('click')});\n};\n\nAdvance.MT.UGC.Photo.initUploadForm = function() {\n    jQuery('<input type=\"hidden\" />')\n        .attr('name', 'gigya_uid')\n        .val(Advance.Gigya.auth.user.UID)\n        .appendTo('form#upload_form');\n    jQuery('<input type=\"hidden\" />')\n        .attr('name', 'gigya_uid_signature')\n        .val(Advance.Gigya.auth.user.UIDSignature)\n        .appendTo('form#upload_form');\n    jQuery('<input type=\"hidden\" />')\n        .attr('name', 'gigya_signature_timestamp')\n        .val(Advance.Gigya.auth.user.signatureTimestamp)\n        .appendTo('form#upload_form');\n};\n\nAdvance.MT.UGC.Video.initUploadForm = function() {\n    jQuery('<input type=\"hidden\" />')\n        .attr('name', 'gigya_uid')\n        .val(Advance.Gigya.auth.user.UID)\n        .appendTo('form#upload_form');\n    jQuery('<input type=\"hidden\" />')\n        .attr('name', 'gigya_uid_signature')\n        .val(Advance.Gigya.auth.user.UIDSignature)\n        .appendTo('form#upload_form');\n    jQuery('<input type=\"hidden\" />')\n        .attr('name', 'gigya_signature_timestamp')\n        .val(Advance.Gigya.auth.user.signatureTimestamp)\n        .appendTo('form#upload_form');\n};\n\nAdvance.MT.UGC.Post.initEntryForm = function() {\n    jQuery('<input type=\"hidden\" />')\n        .attr('name', 'gigya_uid')\n        .val(Advance.Gigya.auth.user.UID)\n        .appendTo('form#create-entry-form');\n    jQuery('<input type=\"hidden\" />')\n        .attr('name', 'gigya_uid_signature')\n        .val(Advance.Gigya.auth.user.UIDSignature)\n        .appendTo('form#create-entry-form');\n    jQuery('<input type=\"hidden\" />')\n        .attr('name', 'gigya_signature_timestamp')\n        .val(Advance.Gigya.auth.user.signatureTimestamp)\n        .appendTo('form#create-entry-form');\n};\n\n//------------------------------------------------------------------------------------\n// Text blog entry functions\n//------------------------------------------------------------------------------------\n/**\n * initEntryCreatePreview\n * @desc - Assigns keyup functions to populate public text blog entry previewer.\n */\ninitEntryCreatePreview = function() {\n   if(jQuery('#create-entry-form').size()) {\n      jQuery('#create-entry-form #entry-title').keyup(function() {\n         jQuery('#postPreviewTitle').html(jQuery('#entry-title').val());\n      });\n      jQuery('#create-entry-form #entry-body').keyup(function() {\n         jQuery('#postPreviewText').html(jQuery('#entry-body').val().replace(/(\\r\\n|\\n)/g,'<br />'));\n      });\n      jQuery('#create-entry-form #entry-tags').keyup(function() {\n         jQuery('#postPreviewTags').html(\"<span>Story tags:</span> \" + jQuery('#entry-tags').val().replace(/,/g,' | '));\n      });\n   }\n};\n\n//------------------------------------------------------------------------------------\n// Tab functions\n//------------------------------------------------------------------------------------\n/* Pages can support multiple instances of tabs.\n * These style tabs are used on the user profile.\n * Basic html form:\n    <div class=\"mt_tabs\">\n        <ul>\n            <li class=\"\"><a href=\"#\" class=\"first\">Tab A</a></li>\n            <li class=\"\"><a href=\"#\" class=\"\">Tab B</a></li>\n            <li class=\"on\"><a href=\"#\" class=\"last\">Tab C</a></li>\n        </ul>\n    </div>\n * Assign \"on\" to the class of the default tab. First and last <li> tags\n * should contain \"first\" and \"last\" classes in their <a> tags.\n */\n//------------------------------------------------------------------------------------\n/**\n * resetTabs\n * @desc - Sets all <li> to \"off\" style.\n * @param ulObj {object} The ul object to set all tabs to the 'off' setting\n */\nresetTabs = function (ulObj) {\n    jQuery(ulObj).children().remove(\"li[class*='mt_tab_active_']\");\n    jQuery.each(jQuery(ulObj).children(), function(){\n        var prevLi = jQuery(this).prev(\"li\");\n        var nextLi = jQuery(this).next(\"li\");\n        jQuery(this).removeClass();\n        jQuery(this).children(\"a\").removeClass();\n        if (!prevLi.length) { jQuery(this).children(\"a\").addClass(\"first\"); }\n        if (!nextLi.length) { jQuery(this).children(\"a\").addClass(\"last\"); }\n    });\n};\n/**\n * setTab\n * @desc - Sets one <li> to \"on\" style.\n * @param liObj {object} The form object whose search values should be checked and sets default values to ''\n */\nsetTab = function (liObj) {\n    var prevLi = jQuery(liObj).prev(\"li\");\n    var nextLi = jQuery(liObj).next(\"li\");\n    jQuery(liObj).addClass(\"mt_tab_active\");\n    if (prevLi.length) {\n        jQuery(liObj).before('<li class=\"mt_tab_active_left\" />');\n        prevLi.children(\"a\").addClass(\"before\");\n    } else {\n        jQuery(liObj).before('<li class=\"mt_tab_active_first\" />');\n        jQuery(liObj).children(\"a\").removeClass(\"first\");\n    }\n    if (nextLi.length) {\n        jQuery(liObj).after('<li class=\"mt_tab_active_right\" />');\n        nextLi.children(\"a\").addClass(\"after\");\n    } else {\n        jQuery(liObj).after('<li class=\"mt_tab_active_last\" />');\n        jQuery(liObj).children(\"a\").removeClass(\"last\");\n    }\n};\n/**\n * initTabs\n * @desc - Sets the default \"on\" tabs in all tabsets on a page (can be called on ready() or within page).\n */\ninitTabs = function () {\n    jQuery.each(jQuery(\".mt_tabs ul\").children(), function(){\n        if (jQuery(this).hasClass(\"on\")) {\n            setTab(this);\n        }\n    }); \n};\n/**\n * initTabsWithClick\n * @desc - Sets the default \"on\" tab in specified tabset on a page and assign state change to click event (can be called on ready() or within page).\n * @param ulObj {object} The ul object to init and assign state change to click event\n */\ninitTabsWithClick = function (ulObj) {\n    jQuery.each(jQuery(ulObj).children(), function(){\n        if (jQuery(this).hasClass(\"on\")) { setTab(this); }\n        jQuery(this).click (function () {\n            resetTabs(jQuery(this).parent());\n            jQuery(this).addClass(\"on\");\n            setTab(jQuery(this));\n        });\n    });\n};\n\n/**\n * popularTabs\n * @desc - Most Popular metrics\n * @param obj {object}\n *        contentUrl {string} \n */\npopularTabs = function (obj, contentUrl) {\n    if((location.href).match(/www|dev\\.advance\\.net/)) {\n        contentUrl = \"/esi_hack.html?env=\" + advBlogSettings.envPrefix + \"&ssurl=\" + escape(contentUrl);\n    }\n    if ( typeof(loadContent) == \"function\" ) {\n        loadContent(contentUrl, \"#PopularStories\");\n    }\n    switchAdvTabs(obj);\n};\n\n/**\n * switchAdvTabs\n * @desc - Tab switcher for right rail tabs\n * @param el {string} \n */\nswitchAdvTabs = function (el) {\n    var tabCells = jQuery(el).parent().parent().children('td');\n    tabCells.not('.first_tab,.last_tab').removeClass();\n    tabCells.removeClass('fix_background');\n        \n    //decide which tab to select\n    if (jQuery(el).parent().is('.first_tab')) { \n        //FIRST tab \n        tabCells.removeClass('last_tab_active');            \n        jQuery(el).parent().addClass('first_tab_active');\n    } else if (jQuery(el).parent().is('.last_tab')) { \n        //LAST tab\n        tabCells.removeClass('first_tab_active');\n        jQuery(el).parent().addClass('last_tab_active');\n        jQuery(el).parent().prev().addClass('fix_background');  //remove the right border and extra padding on tab before this\n    } else if (jQuery(el).parent().not('.first_tab,.last_tab')) { \n        //MIDDLE tabs\n        tabCells.removeClass('first_tab_active last_tab_active');\n        jQuery(el).parent().addClass('tab_active');\n        jQuery(el).parent().prev().addClass('fix_background');//remove the right border and extra padding on tab before this        \n    } else {\n        //nothing\n    }\n};\n\nfunction conditional_block(cond, id) {\n    if ( DEBUG && !jQuery.browser.msie ) \n        console.debug(arguments.callee, arguments);\n\n    var true_block = document.getElementById(id);\n    var false_block = document.getElementById(id + '_else');\n    if (cond) {\n        if (true_block) {\n            var display = true_block.getAttribute('mt:display_style');\n            if (!display && false_block)\n                display = false_block.getAttribute('mt:display_style');\n            if (!display) display = '';\n            true_block.style.display = display;\n            DOM.removeClassName(true_block, 'hidden');\n        }\n        if (false_block) {\n            false_block.style.display = 'none';\n            DOM.addClassName(false_block, 'hidden');\n        }\n    } \n    else {\n        if (true_block) {\n            true_block.style.display = 'none';\n            DOM.addClassName(true_block, 'hidden');\n        }\n        if (false_block) {\n            var display = false_block.getAttribute('mt:display_style');\n\n            if (!display && true_block)\n                display = false_block.getAttribute('mt:display_style');\n\n            if (!display) display = '';\n\n            false_block.style.display = display;\n            DOM.removeClassName(false_block, 'hidden');\n        }\n    }\n}\n\n/***\n * Utility functions\n */\n\n//// MT Util Fuctions\n// A utility function for assigning/adding handlers to window events.\nAdvance.MT.util.attachEventHandler = function(eventName,func) {\n    var onEventName = 'on' + eventName;\n    var old = window[onEventName];\n    if( typeof old != 'function' )\n        window[onEventName] = func;\n    else {\n        window[onEventName] = function( evt ) {\n            old( evt );\n            return func( evt );\n        };\n    }\n}\n\n// Calls the event named, if there are handlers for it.\nAdvance.MT.util.fireEvent = function(eventName,param) {\n    if ( DEBUG && !jQuery.browser.msie ) \n        console.debug(arguments.callee, arguments);\n\n    var fn = window['on' + eventName];\n    if (typeof fn == 'function') return fn(param);\n    return;\n}\n\n\n\n//// misc\n// jQuery object serialization\njQuery.fn.serializeObject = function() {\n    var o = {};\n    var a = this.serializeArray();\n    jQuery.each(a, function() {\n        if (o[this.name] !== undefined) {\n            if (!o[this.name].push) {\n                o[this.name] = [o[this.name]]